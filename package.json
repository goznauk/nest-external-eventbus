{
  "name": "nest-external-eventbus",
  "version": "0.0.2",
  "description": "external eventbus module for @nestjs/cqrs",
  "author": { "name": "goznauk", "email": "goznauk@naver.com", "url": "https://github.com/goznauk" },
  "contributors": [],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goznauk/nest-external-eventbus.git"
  },
  "bugs": {
    "url": "https://github.com/goznauk/nest-external-eventbus/issues"
  },
  "files": [
    "dist",
    "packages"
  ],
  "scripts": {
    "commit": "git-cz",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "build:test": "npm build && npm pack"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test && npm run lint",
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "engines": {
    "node": ">= 14"
  },
  "peerDependencies": {
    "@nestjs/core": "^9",
    "@nestjs/common": "^9",
    "@nestjs/cqrs": "^9",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7"
  },
  "devDependencies": {
    "@nestjs/cli": "^9",
    "@nestjs/core": "^9",
    "@nestjs/common": "^9",
    "@nestjs/cqrs": "^9",
    "@nestjs/schematics": "^9",
    "@nestjs/testing": "^9",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^9",
    "ioredis": "^5.2.1",
    "redis": "^4.2.0",
    "rxjs": "^7"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "eventbus",
    "microservice",
    "cqrs",
    "nestjs",
    "redis"
  ]
}
